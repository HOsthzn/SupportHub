<div class="d-flex justify-content-between">
    <a class="btn btn-primary" href="/accessManagement/roles/create"><i class="bi bi-plus"></i>Add</a>
    <button class="btn btn-info float-end"><i class="bi bi-chevron-left"></i>Details</button>
</div>
<table>
    <thead>
    <tr>
        <th></th>
        <th>Name</th>
    </tr>
    </thead>
    <tbody>
    <% for(let organization of data){ %>
        <tr>
            <td>
                <input type="checkbox" name="selectOrganization" value="<%= organization.id %>">
            </td>
            <td>
                <a href="/accessManagement/roles/details/<%= organization.id %>"><%= organization.settings.name %></a>
            </td>
        </tr>
    <% } %>
    </tbody>
</table>

